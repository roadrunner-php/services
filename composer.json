    "require": {
        "php": ">=8.1",
        "roadrunner-php/roadrunner-api-dto": "^1.1",
        "spiral/roadrunner": "^2023.2",
        "spiral/goridge": "^4.0",
        "google/protobuf": "^3.7"
    },
    "autoload": {
        "psr-4": {
            "Spiral\\RoadRunner\\Services\\": "src"
        }
    },
    "require-dev": {
        "mockery/mockery": "^1.5",
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": ">=5.8"
    },
    "autoload-dev": {
        "psr-4": {
            "Spiral\\RoadRunner\\Services\\Tests\\": "tests"
        }
    },
    "scripts": {
        "tests": "phpunit",
        "psalm": "psalm --no-cache"
